{
  "name": "jest",
  "version": "1.0.0",
  "description": "NodeJS API TDD Jest",
  "jest": {
    "verbose": true
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/server.js",
    "import:csv": "mongoimport --uri mongodb://localhost:27017/salam-api --collection sourates --type csv --file coranComplet.csv --headerline",
    "start": "cross-env NODE_ENV=production npm run build && node ./dist/server.js",
    "clean": "rm -rf build && mkdir build",
    "build": "npm run clean && npx babel src --out-dir dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.15.3",
    "babel-jest": "^27.0.6",
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "nodemon": "^2.0.12"
  },
  "dependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jest": "^27.0.6",
    "jsonwebtoken": "^8.5.1",
    "mongoosastic": "^4.6.0",
    "mongoose": "^5.13.8",
    "morgan": "^1.10.0",
    "supertest": "^6.1.3",
    "validator": "^13.6.0"
  },
  "transform": {
    "\\.[jt]sx?$": "babel-jest"
  }
}
